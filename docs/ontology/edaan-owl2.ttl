#################################################################
# Object Properties
#################################################################

# NOTE: dct:subject and dcat:theme are imported; we don't re-declare them.

:compatibleWith a owl:ObjectProperty , owl:SymmetricProperty ;
  rdfs:domain rdfs:Resource ; rdfs:range rdfs:Resource ;
  rdfs:label "compatible with"@en ;
  rdfs:comment "Curated compatibility between resources (dataset↔app, app↔app, profile↔profile)."@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:conformsToProfile a owl:ObjectProperty ;
  rdfs:domain ids:Resource ; rdfs:range :DataProfile ;
  rdfs:label "conforms to profile"@en ;
  rdfs:comment "States that a resource conforms to a specific DataProfile; complement to dct:conformsTo for schemas/standards."@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

# Bridge: IDSA DataApp ↔ BIGOWL Component (direct, no Implementation)
:implementsComponent a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ;
  rdfs:range  bigwf:Component ;
  rdfs:label "implements component"@en ;
  rdfs:comment "Links an IDSA DataApp to the BIGOWL workflow Component it implements."@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:isImplementedBy a owl:ObjectProperty ;
  owl:inverseOf :implementsComponent ;
  rdfs:domain bigwf:Component ;
  rdfs:range  ids:DataApp ;
  rdfs:label "is implemented by"@en ;
  rdfs:comment "Inverse of edaan:implementsComponent."@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

# Convenience: bring BIGOWL parameters onto the DataApp via the linked Component
:hasParameter a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ;
  rdfs:range  bigwf:Parameter ;
  rdfs:label "has parameter"@en ;
  rdfs:comment "Convenience link: a DataApp has the BIGOWL parameters declared by the Component it implements."@en ;
  owl:propertyChainAxiom ( :implementsComponent bigwf:hasParameter ) ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:consumesResource a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ;
  rdfs:range  ids:Representation ;
  rdfs:label "consumes representation"@en ;
  rdfs:comment "Indicates that the DataApp consumes a specific technical representation (format/encoding)."@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:producesResource a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ;
  rdfs:range  ids:Representation ;
  rdfs:label "produces representation"@en ;
  rdfs:comment "Indicates that the DataApp produces a specific technical representation (format/encoding)."@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:declaresAlgorithm a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ; rdfs:range bigalg:Algorithm ;
  rdfs:label "declares algorithm"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:declaresDataClass a owl:ObjectProperty ;
  rdfs:domain :DataProfile ; rdfs:range bigdat:Data ;
  rdfs:label "declares data class"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:declaresObservedProperty a owl:ObjectProperty ;
  rdfs:domain :DataProfile ; rdfs:range skos:Concept ;
  rdfs:label "declares observed property"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:hasDomainSector a owl:ObjectProperty ;
  rdfs:subPropertyOf dcat:theme ;
  rdfs:domain ids:Resource ; rdfs:range skos:Concept ;
  rdfs:label "has domain sector"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:hasOffer a owl:ObjectProperty ;
  rdfs:domain ids:Resource ; rdfs:range ids:ContractOffer ;
  rdfs:label "has offer"@en ;
  rdfs:comment "Associates a resource with an IDS ContractOffer (usage policy, pricing, etc.)."@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:pricePlan a owl:ObjectProperty ;
  rdfs:domain ids:ContractOffer ; rdfs:range :PricePlan ;
  rdfs:label "price plan"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:profileCRSRef a owl:ObjectProperty ;
  rdfs:domain :DataProfile ; rdfs:range rdfs:Resource ;
  rdfs:label "profile CRS (IRI)"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:realizesWorkflow a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ; rdfs:range opmw:WorkflowTemplate ;
  rdfs:label "realizes workflow"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:requiresProfile a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ; rdfs:range :DataProfile ;
  rdfs:label "requires profile"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:producesProfile a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ; rdfs:range :DataProfile ;
  rdfs:label "produces profile"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:solvesProblem a owl:ObjectProperty ;
  rdfs:domain ids:DataApp ; rdfs:range bigprob:Problem ;
  rdfs:label "solves problem"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:spatialGranularityConcept a owl:ObjectProperty ;
  rdfs:domain ids:Resource ; rdfs:range skos:Concept ;
  rdfs:label "spatial granularity (concept)"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

:topic a owl:ObjectProperty ;
  rdfs:subPropertyOf dct:subject ;
  rdfs:domain ids:Resource ; rdfs:range skos:Concept ;
  rdfs:label "topic"@en ;
  rdfs:isDefinedBy <https://w3id.org/EDAAnOWL/> .

