@prefix : <https://w3id.org/EDAAnOWL/> .
@prefix ex: <https://w3id.org/EDAAnOWL/examples/minimal/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/idsa/core/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix bigalg: <https://w3id.org/BIGOWLAlgorithms/> .
@prefix bigprob: <https://w3id.org/BIGOWLProblems/> .
@prefix bigdat: <https://w3id.org/BIGOWLData/> .

# --- Prefijos para los vocabularios SKOS ---
@prefix sector: <https://w3id.org/EDAAnOWL/vocabularies/sector-scheme#> .
@prefix dtype: <https://w3id.org/EDAAnOWL/vocabularies/datatype-scheme#> .
@prefix obs: <https://w3id.org/EDAAnOWL/vocabularies/observed-properties#> .

ex:MinimalDataResource a ids:DataResource ;
    dct:title "Minimal Test Dataset"@en ;
    :hasDomainSector sector:agriculture ;
    :conformsToProfile ex:MinimalProfile ;
    :supportContact "test@example.org" .

ex:MinimalProfile a :DataProfile ;
    dct:title "Minimal Data Profile"@en ;
    :declaresDataClass ex:TestDataClass ;
    :declaresObservedProperty obs:yield .

ex:TestDataClass a bigdat:Data ;
    rdfs:label "Test Data Class"@en .

ex:MinimalDataApp a ids:DataApp ;
    dct:title "Minimal Test App"@en ;
    :declaresAlgorithm ex:TestAlgorithm ;
    :solvesProblem ex:TestProblem ;
    :hasDomainSector sector:agriculture ;
    :requiresProfile ex:MinimalProfile .

ex:TestAlgorithm a bigalg:Algorithm ;
    rdfs:label "Test Algorithm"@en .

ex:TestProblem a bigprob:Problem ;
    rdfs:label "Test Problem"@en .

ex:MinimalOffer a ids:ContractOffer ;
    dct:title "Minimal Test Offer"@en ;
    odrl:permission [
        odrl:action odrl:use ;
        odrl:constraint [
            odrl:leftOperand odrl:purpose ;
            odrl:operator odrl:eq ;
            odrl:rightOperand "testing"
        ]
    ] .

ex:MinimalMetric a :PerformanceMetric ;
    :metricName "Test Accuracy" ;
    :metricValue 0.95 ;
    :metricUnit "percentage" ;
    :computedAt "2025-01-20T10:00:00Z"^^xsd:dateTime .